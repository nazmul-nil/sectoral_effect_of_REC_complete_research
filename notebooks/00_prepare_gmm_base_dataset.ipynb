    "import pandas as pd\n",
    "import numpy as np\n",
    "\n",
    "# Load the dataset\n",
    "df = pd.read_csv(\"../datasets/enhanced_growth_rates_emissions_energy_prod_income_level_country_df.csv\")\n",
    "\n",
    "print(\"Dataset loaded successfully!\")\n",
    "print(f\"Shape: {df.shape}\")\n",
    "\n",
    "# Step 1: Examine available series\n",
    "print(\"\\n\" + \"=\"*60)\n",
    "print(\"STEP 1: Examining available series indicators\")\n",
    "print(\"=\"*60)\n",
    "\n",
    "print(f\"Total unique series: {df['Series Name'].nunique()}\")\n",
    "print(f\"Total unique series codes: {df['Series Code'].nunique()}\")\n",
    "\n",
    "# Display all available series with their codes\n",
    "series_info = df[['Series Name', 'Series Code']].drop_duplicates().sort_values('Series Name')\n",
    "print(f\"\\nAll available series:\")\n",
    "for idx, row in series_info.iterrows():\n",
    "    print(f\"• {row['Series Code']}: {row['Series Name']}\")\n",
    "\n",
    "# Step 2: Define relevant series mapping\n",
    "print(\"\\n\" + \"=\"*60)\n",
    "print(\"STEP 2: Mapping relevant series for research\")\n",
    "print(\"=\"*60)\n",
    "\n",
    "# Define which series codes correspond to our research variables\n",
    "# You'll need to update these based on the actual series codes shown above\n",
    "relevant_series_mapping = {\n",
    "    # Economic Growth Indicators\n",
    "    'NY.GDP.MKTP.KD.ZG': 'GDPgrowth',\n",
    "    'NV.AGR.TOTL.KD.ZG': 'AgriGrowth', \n",
    "    'NV.IND.TOTL.KD.ZG': 'IndGrowth',\n",
    "    'NV.SRV.TOTL.KD.ZG': 'ServGrowth',\n",
    "    \n",
    "    # Energy & Environment Indicators  \n",
    "    'EG.ELC.RNEW.ZS': 'REC',  # Renewable energy consumption (% of total)\n",
    "    'EG.USE.COMM.GD.PP.KD': 'EI',  # Energy intensity\n",
    "    'EN.ATM.CO2E.PC': 'CO2pc',  # CO2 emissions per capita\n",
    "    'EG.ELC.ACCS.ZS': 'AccessElec',  # Access to electricity\n",
    "    \n",
    "    # Additional indicators\n",
    "    'EN.ATM.PM25.MC.M3': 'PM2.5',  # PM2.5 air pollution\n",
    "    'NY.GDP.MKTP.PP.CD': 'GDP_PPP',  # GDP PPP\n",
    "    \n",
    "    # Add more mappings as needed based on available series\n",
    "}\n",
    "\n",
    "# Check which of our desired series are actually available\n",
    "available_series = set(df['Series Code'].unique())\n",
    "found_series = {}\n",
    "missing_series = {}\n",
    "\n",
    "for code, name in relevant_series_mapping.items():\n",
    "    if code in available_series:\n",
    "        found_series[code] = name\n",
    "        print(f\"✅ Found: {code} -> {name}\")\n",
    "    else:\n",
    "        missing_series[code] = name\n",
    "        print(f\"❌ Missing: {code} -> {name}\")\n",
    "\n",
    "print(f\"\\nSummary: {len(found_series)} found, {len(missing_series)} missing\")\n",
    "\n",
    "# Step 3: Filter dataset to keep only relevant series\n",
    "print(\"\\n\" + \"=\"*60)\n",
    "print(\"STEP 3: Filtering dataset to relevant series\")\n",
    "print(\"=\"*60)\n",
    "\n",
    "# Filter to keep only the series we found\n",
    "relevant_codes = list(found_series.keys())\n",
    "df_filtered = df[df['Series Code'].isin(relevant_codes)].copy()\n",
    "\n",
    "print(f\"Filtered dataset shape: {df_filtered.shape}\")\n",
    "print(f\"Remaining series: {df_filtered['Series Code'].nunique()}\")\n",
    "\n",
    "# Step 4: Transform to long format\n",
    "print(\"\\n\" + \"=\"*60)\n",
    "print(\"STEP 4: Transforming to long format\")\n",
    "print(\"=\"*60)\n",
    "\n",
    "# Identify year columns\n",
    "year_cols = [col for col in df_filtered.columns if '_' in col and col.split('_')[0].isdigit()]\n",
    "print(f\"Year columns: {len(year_cols)} from {year_cols[0]} to {year_cols[-1]}\")\n",
    "\n",
    "# Melt to long format\n",
    "id_vars = ['Country Name', 'Country Code', 'Series Name', 'Series Code', 'Income Level']\n",
    "df_long = pd.melt(df_filtered, \n",
    "                  id_vars=id_vars,\n",
    "                  value_vars=year_cols, \n",
    "                  var_name='year_col', \n",
    "                  value_name='value')\n",
    "\n",
    "# Extract year from column names\n",
    "df_long['year'] = df_long['year_col'].str.split('_').str[0].astype(int)\n",
    "df_long = df_long.drop('year_col', axis=1)\n",
    "\n",
    "print(f\"Long format shape: {df_long.shape}\")\n",
    "\n",
    "# Step 5: Pivot to get variables as columns\n",
    "print(\"\\n\" + \"=\"*60)\n",
    "print(\"STEP 5: Creating final panel structure\")\n",
    "print(\"=\"*60)\n",
    "\n",
    "# Pivot with Series Code as columns\n",
    "df_panel = df_long.pivot_table(\n",
    "    index=['Country Name', 'Country Code', 'year', 'Income Level'],\n",
    "    columns='Series Code', \n",
    "    values='value',\n",
    "    aggfunc='first'\n",
    ").reset_index()\n",
    "\n",
    "# Flatten column names\n",
    "df_panel.columns.name = None\n",
    "\n",
    "print(f\"Panel dataset shape: {df_panel.shape}\")\n",
    "\n",
    "# Step 6: Rename columns to meaningful names\n",
    "print(\"\\n\" + \"=\"*60) \n",
    "print(\"STEP 6: Applying meaningful variable names\")\n",
    "print(\"=\"*60)\n",
    "\n",
    "# Rename columns using our mapping\n",
    "for code, name in found_series.items():\n",
    "    if code in df_panel.columns:\n",
    "        df_panel = df_panel.rename(columns={code: name})\n",
    "        print(f\"Renamed: {code} -> {name}\")\n",
    "\n",
    "# Step 7: Create final GMM base dataset\n",
    "print(\"\\n\" + \"=\"*60)\n",
    "print(\"STEP 7: Creating GMM base dataset\")\n",
